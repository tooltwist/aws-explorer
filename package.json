{
  "name": "aws-explorer",
  "version": "1.0.8",
  "description": "AWS visualisation tool",
  "author": "Philip Callender <philip.callender@tooltwist.com>",
  "main": "index.js",
  "scripts": {
    "dev": "backpack dev",
    "build": "nuxt build && backpack build",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "patch-release": "npm version patch && npm publish && git push --follow-tags"
  },
  "bin": {
    "aws-explorer": "bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tooltwist/aws-explorer.git"
  },
  "keywords": [
    "aws",
    "ec2",
    "vpc"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tooltwist/aws-explorer/issues"
  },
  "homepage": "https://github.com/tooltwist/aws-explorer#readme",
  "dependencies": {
    "@nuxtjs/bulma": "^1.1.0",
    "aws-regions": "^1.3.1",
    "aws-sdk": "^2.175.0",
    "axios": "^0.16.2",
    "cross-env": "^5.0.1",
    "exec-sh": "^0.2.1",
    "express": "^4.15.3",
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "nuxt": "^1.0.0-rc3",
    "source-map-support": "^0.4.15",
    "vis": "^4.21.0",
    "vue-blu": "^0.1.9"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "backpack-core": "^0.4.3",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "node-sass": "^4.7.2",
    "pug": "^2.0.0-beta6",
    "pug-loader": "^2.3.0",
    "sass-loader": "^6.0.6"
  }
